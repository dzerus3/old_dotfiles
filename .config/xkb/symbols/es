// Draws on my colemak_dh layout, but replaces the brackets and -_ with ñÑ¡¿áä

default partial alphanumeric_keys
xkb_symbols "colemak_dh" {

    include "us(colemak_dh)"

    name[Group1]="Spanish";

    key <BKSL>  { [dead_acute, dead_diaeresis, braceleft,    dead_caron ]	};
    key <AD11>  { [ntilde,     Ntilde,         dead_tilde,   dead_doubleacute ]	};
    key <AD12>  { [exclamdown, questiondown,   dead_cedilla, dead_ogonek ]	};
    key <AE11>  { [minus,      underscore,     endash,       emdash ] };

    include "level3(ralt_switch)"
};

